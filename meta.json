{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "shannon:hand-eye-test",
  "visibility": "private",
  "url": "https://github.com/shannonbradshaw/hand-eye-test",
  "description": "Validates hand-eye calibration by detecting objects via point cloud segmentation and commanding a robot arm to pick them up.",
  "markdown_link": "README.md",
  "entrypoint": "bin/hand-eye-test",
  "build": {
    "build": "make module.tar.gz",
    "setup": "make setup",
    "path": "module.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64",
      "darwin/arm64"
    ]
  },
  "models": [
    {
      "api": "rdk:service:generic",
      "model": "shannon:hand-eye-test:calibration-tester"
    }
  ]
}
